{
	"name": "beauty-tour-solution",
	"version": "1.0.0",
	"keywords": [],
	"author": "gooday6983@gmail.com",
	"scripts": {
		"dev": "pnpm --filter @bts/web dev",
		"sst:dev": "pnpm --filter @bts/web sst:dev",
		"build": "pnpm --filter @bts/infra build && pnpm --filter @bts/web build",
		"build:infra": "pnpm --filter @bts/infra build",
		"test": "vitest run",
		"test:core": "pnpm --filter @bts/core test",
		"preview": "pnpm --filter @bts/web preview",
		"lint": "biome check --write && pnpm run -r lint",
		"format": "biome format --write && pnpm run -r format",
		"check": "biome check && pnpm run -r check",
		"dev-remove": "pnpm --filter @bts/web dev-remove",
		"prod-deploy": "pnpm --filter @bts/web prod-deploy",
		"prod-remove": "pnpm --filter @bts/web prod-remove"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.5",
		"vitest": "^3.2.4"
	}
}
