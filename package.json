{
	"name": "beauty-tour-solution",
	"version": "0.1.0",
	"keywords": [],
	"author": "gooday6983@gmail.com",
	"scripts": {
		"dev": "pnpm --filter @bts/web dev",
		"build": "pnpm --filter @bts/web build",
		"test": "vitest run",
		"test:core": "pnpm --filter @bts/core test",
		"lint": "biome check --write && pnpm run -r lint",
		"format": "biome format --write && pnpm run -r format",
		"check": "biome check && pnpm run -r check",
		"deploy-agentcore": "./scripts/deploy-agentcore.sh",
		"prod-deploy": "pnpm --filter @bts/agentcore prod-deploy && pnpm --filter @bts/web prod-deploy",
		"prod-remove": "pnpm --filter @bts/agentcore prod-remove && pnpm --filter @bts/web prod-remove"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.5",
		"vitest": "^3.2.4"
	}
