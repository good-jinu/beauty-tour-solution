AWS_ACCESS_KEY_ID="your_access_key_id"
AWS_SECRET_ACCESS_KEY="your_secret_access_key"
APP_AWS_REGION="us-east-1"

# Domain for the deployed application
WEB_DOMAIN="your_domain.com"

# Event Tracking Configuration
EVENT_TRACKING_ENABLED="true"
EVENT_BATCHING_ENABLED="true"
EVENT_BATCH_SIZE="10"
EVENT_BATCH_TIMEOUT="5000"
RATE_LIMIT_PER_MINUTE="100"

# Cookie Configuration
GUEST_COOKIE_MAX_AGE="2592000"
GUEST_COOKIE_SECURE="true"
GUEST_COOKIE_SAME_SITE="lax"

# Privacy and Security
EVENT_TRACKING_ENABLE_OPT_OUT="true"
EVENT_TRACKING_SANITIZE_DATA="true"
EVENT_TRACKING_LOG_LEVEL="info"

# Pulumi backend
PULUMI_BACKEND_S3="s3://statestore/bts?region=us-east-1"
PULUMI_CONFIG_PASSPHRASE="passphrase"