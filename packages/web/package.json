{
	"name": "@bts/web",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "dotenv -e ../agentcore/.env.agentcore -- vite build",
		"dev": "dotenv -e ../agentcore/.env.agentcore -- vite dev",
		"prepare": "svelte-kit sync || echo ''",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"sst:dev": "sst dev --stage development",
		"dev-remove": "sst remove --stage development",
		"prod-deploy": "sst deploy --stage production",
		"prod-remove": "sst remove --stage production"
	},
	"devDependencies": {
		"@internationalized/date": "^3.10.0",
		"@lucide/svelte": "^0.544.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/vite": "^4.0.0",
		"bits-ui": "^2.11.4",
		"clsx": "^2.1.1",
		"dotenv-cli": "^10.0.0",
		"jsdom": "^27.0.1",
		"mode-watcher": "^1.1.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-sonner": "^1.0.5",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^3.1.1",
		"tailwindcss": "^4.0.0",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4",
		"vite-plugin-env-compatible": "^2.0.1"
	},
	"dependencies": {
		"@bts/core": "workspace:*",
		"@bts/infra": "workspace:*",
		"marked": "^16.3.0",
		"sst": "3.17.14",
		"svelte-kit-sst": "2.43.5"
	}
}
